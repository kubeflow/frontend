{
  "name": "frontend",
  "version": "0.1.0",
  "description": "Shared libraries and components used by Kubeflow frontend projects.",
  "private": true,
  "main": "build/lib/index.js",
  "dependencies": {
    "cross-env": "^6.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "apis:metadata": "node ./gen_grpc_web_protos.js",
    "build": "npx tsc -p tsconfig.lib.json",
    "eject": "cross-env EXTEND_ESLINT=true react-scripts eject",
    "postinstall": "npm run build",
    "test": "cross-env TZ='America/New_York' EXTEND_ESLINT=true react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "./",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "proxy": "http://localhost:8080",
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "typescript": "^3.7.4"
  }
}
